{
    "manifest_version": 2,
    "name": "찾아드림",
    "description": "조각난 방문기록들을 모아드립니다.",
    "version": "1.0",
    "browser_action": {
        "default_icon": "img/ic_default.png",
        "default_popup": "main.html"
    },
    "web_accessible_resources": ["main.html"],
    "permissions": [
        "tabs",
        "notifications",
        "activeTab",
        "storage",
        "history",
        "<all_urls>"
    ]
}